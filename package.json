{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mochi-raycast-extension",
  "title": "mochi-raycast-extension",
  "description": "Uses mochi pay api to fetch token info and other stuff",
  "icon": "command-icon.png",
  "author": "anujgupta",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-token",
      "title": "Search Token",
      "description": "Searches Token based on input",
      "mode": "view"
    },
    {
      "name": "search-nft-collections",
      "title": "Search NFT Collections",
      "description": "Searches NFT Collections",
      "mode": "view"
    },
    {
      "name": "market-data",
      "title": "View Market Data",
      "description": "",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
